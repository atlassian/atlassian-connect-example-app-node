{
  "name": "@atlassian/atlassian-connect-sample-app-node",
  "version": "1.0.0",
  "repository": "git@github.com:atlassian/atlassian-connect-sample-app-node.git",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf src/**/*.js db.json",
    "build": "tsc --project .",
    "postinstall": "ts-node setup.ts",
    "prestart": "ts-node prestart.ts",
    "start": "tsnd src/server.ts",
    "test:e2e": "run-s test:e2e:install test:e2e:start",
    "test:e2e:install": "playwright install",
    "test:e2e:start": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 yarn test:e2e --headed"
  },
  "dependencies": {
    "@commonify/lowdb": "^3.0.0",
    "atlassian-jwt": "^2.0.2",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mustache-express": "^1.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@types/express": "^4.17.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5"
  }
}
