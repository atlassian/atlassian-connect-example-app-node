{
  "name": "@atlassian/node-connect-example",
  "version": "1.0.0",
  "repository": "git@github.com:atlassian/atlassian-connect-example-app-node.git",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "precommit": "run-p lint build",
    "clean": "rimraf src/**/*.js test/**/*.js build",
    "clean:db": "rimraf db.json",
    "lint": "eslint . --ext .ts,.tsx",
    "build": "tsc --project .",
    "postinstall": "ts-node postinstall.ts",
    "prestart": "run-p clean prestart:wait",
    "prestart:wait": "ts-node prestart.ts",
    "start": "tsnd --watch=.env,**/*.squirrelly --inspect=0.0.0.0:9229 --respawn --transpile-only -- src/server.ts",
    "test:e2e": "run-s test:e2e:install test:e2e:start",
    "test:e2e:install": "playwright install",
    "test:e2e:start": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 yarn test:e2e --headed"
  },
  "dependencies": {
    "@commonify/lowdb": "^3.0.0",
    "@types/marked": "^4.0.8",
    "atlassian-jwt": "^2.0.2",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "marked": "^4.2.12",
    "squirrelly": "^8.0.8",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@types/express": "^4.17.16",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5"
  },
	"engines": {
		"node": ">= 16",
		"yarn": "^1.22.0"
	},
	"volta": {
		"node": "18.15.0",
		"yarn": "1.22.18"
	}
}
