{{@extendsFile('./layout.squirrelly')}}
    {{#title}}
    Webhook logs
    {{#content}}
    {{@includeFile('./partial/sidebar.squirrelly') /}}
    <div>
        {{@includeFile('./partial/header.squirrelly') /}}
        <div id="webhooks" data-page-content="{{ it.pageContent.toString() }}"></div>

        <div class="flexed">
            <div>
                <a class="aui-button" onclick="AP.navigator.reload();">
                    <span class="aui-icon aui-icon-small aui-iconfont-refresh">Refresh</span>
                    <span>Refresh</span>
                </a>
            </div>
        </div>
        <br>
        <div class="logs-container">
            {{@if(it.logs)}}
            {{@each(it.logs) => log}}
            <pre>{{log.message}} -----> {{log.data}}</pre>
            {{/each}}
            {{#else}}
            <pre>No logs yet!</pre>
            {{/if}}
        </div>
    </div>
{{/extendsFile}}
